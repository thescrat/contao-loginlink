{
    "name": "thescrat/contao-loginlink",
    "description": "Mitglied wird mit einem Key / Token eingeloggt.",
    "type": "contao-bundle",
    "license": "MIT",
    "authors":[
        {
            "name":"Michael Fleischmann"
        }
    ],
    "support": {
        "issues": "https://github.com/thescrat/contao-loginlink/issues",
        "source": "https://github.com/thescrat/contao-loginlink",
        "forum": "https://community.contao.org"
    },
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "contao/core-bundle": "^4.13",
        "psr/log": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Thescrat\\LoginLinkBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Thescrat\\LoginLinkBundle\\ContaoManager\\Plugin"
    }
}
